    "while True:\n",
    "    a, op, b = input().split()\n",
    "    a = int(a)\n",
    "    b = int(b)\n",
    "    op = str(op)\n",
    "    if op == \"+\":\n",
    "        print(a+b)\n",
    "    elif op == \"-\":\n",
    "        print(a-b)\n",
    "    elif op == \"*\":\n",
    "        print(a*b)\n",
    "    elif op == \"/\":\n",
    "        print(a//b)\n",
    "    elif op == \"?\":\n",
    "        break"
   ]
