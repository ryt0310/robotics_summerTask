   "source": [
    "import math\n",
    "a = list(map(float,input().split()))\n",
    "θ = math.radians(a[2])\n",
    "S = 1/2*(a[0]*a[1]*math.sin(θ))\n",
    "L = a[0]+a[1]+math.sqrt(a[0]*a[0]+a[1]*a[1]-2*a[0]*a[1]*math.cos(θ))\n",
    "h = a[1]*math.sin(θ)\n",
    "print(\"{:.5f}\".format(S))\n",
    "print(\"{:.5f}\".format(L))\n",
    "print(\"{:.5f}\".format(h))"
   ]
