    "import math\n",
    "while True:\n",
    "    n = int(input())\n",
    "    if n==0:\n",
    "        break\n",
    "    s = list(map(int,input().split()))\n",
    "    m = 0\n",
    "    a1 = 0\n",
    "    for i in s:\n",
    "        m = m+i\n",
    "    m = m/n\n",
    "    for i in s:\n",
    "        a1 = a1+((i-m)*(i-m))\n",
    "    a2 = a1/n\n",
    "    a = math.sqrt(a2)\n",
    "    print(a)"
   ]
